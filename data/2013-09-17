 besser82	OPT=OFF -DBUNDLE_JSON=OFF -DENABLE_TESTING=ON -DUSE_SVMLIGHT=OFF -DCmdLineStatic=ON -DLuaModular=ON -DOctaveModular=ON -DPythonModular=ON -DRubyModular=ON ..
